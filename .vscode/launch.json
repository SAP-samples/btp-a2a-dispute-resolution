{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds serve",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": ["serve", "--with-mocks", "--in-memory?"],
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "command": "yarn watch",
      "name": "watch - agent-catalog",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceRoot}/agent-catalog",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "command": "yarn watch",
      "name": "watch - sap-agent-builder-a2a",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceRoot}/agents/sap-agent-builder-a2a/agent-builder-a2a-agent-connector",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
