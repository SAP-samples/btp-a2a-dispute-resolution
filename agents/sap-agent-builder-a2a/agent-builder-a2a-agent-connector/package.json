{
    "name": "sap-agent-builder-a2a-agent-connector",
    "version": "1.0.0",
    "description": "Wraps the SAP Dispute Resolution Agent, which runs on Business Agent Foundation (BAF), to use the A2A protocol",
    "repository": "https://github.tools.sap/platform-adoption-advisory/a2a-dispute-resolution",
    "license": "UNLICENSED",
    "private": true,
    "engines": {
        "node": "22.x.x"
    },
    "dependencies": {
        "@a2a-js/sdk": "^0.2.2",
        "@cap-js/ord": "^1.3.3",
        "@sap/cds": "^9",
        "axios": "^1.9.0",
        "bcrypt": "^6.0.0",
        "express": "^4"
    },
    "devDependencies": {
        "@cap-js/cds-typer": "0.33.1",
        "@cap-js/cds-types": "^0.10.0",
        "@sap/cds-dk": "^9.1.0",
        "@types/node": "^22.0.0",
        "prettier": "^3.5.3",
        "tsx": "^4",
        "typescript": "^5"
    },
    "scripts": {
        "start": "cds-serve",
        "watch": "cds-tsx w --profile hybrid",
        "build": "mbt build -t mta_archives --mtar sap-agent-builder-a2a-agent-connector.tar",
        "deploy-only": "cf deploy mta_archives/sap-agent-builder-a2a-agent-connector.tar",
        "deploy": "npm run build && npm run deploy-only"
    },
    "prettier": {
        "singleQuote": false,
        "trailingComma": "none",
        "printWidth": 120,
        "tabWidth": 4
    },
    "imports": {
        "#cds-models/*": "./@cds-models/*/index.js"
    }
}
