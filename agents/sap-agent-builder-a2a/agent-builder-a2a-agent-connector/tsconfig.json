{
    "compilerOptions": {
        "target": "ES2023",
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "outDir": "./gen/srv/srv",
        "skipLibCheck": true,
        "sourceMap": true,
        "allowJs": true,
        "strict": false,
        "strictNullChecks": false,
        "strictPropertyInitialization": false,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "paths": {
            "#cds-models/*": ["./@cds-models/*/index.ts"]
        }
        //"types": ["./typings.d.ts", "@cap-js/cds-types"]
        //"typeRoots": ["./node_modules/@types", "./node_modules/@cap-js"]
    },
    "include": ["./srv/**/*"]
}
